mat2 rotate2d(float angle){
    float s = sin(angle);
    float c = cos(angle);
    return mat2(c, -s, s, c);
}

float min_dist_to_poly(vec2 p, vec2 poly[N]){
    float d = 1e10;
    for(int i = 0; i < N; i++){
        vec2 a = poly[i];
        vec2 b = poly[(i+1)%N];
        vec2 pa = p - a;
        vec2 ba = b - a;
        float h = clamp(dot(pa, ba)/dot(ba, ba), 0.0, 1.0);
        d = min(d, length(pa - ba*h));
    }
    return d;
}

bool is_inside(vec2 p, vec2 poly[N]){
    int count = 0;
    for(int i = 0; i < N; i++){
        vec2 a = poly[i];
        vec2 b = poly[(i+1)%N];
        if(((a.y > p.y) != (b.y > p.y)) && (p.x < a.x + (b.x - a.x) * (p.y - a.y) / (b.y - a.y + 1e-5))){
            count++;
        }
    }
    return (count % 2) == 1;
}

fn pixel() -> vec4 {
    // pos is the fragment position in [0,1]x[0,1]
    vec2 p = pos * 48.0; // scale to SVG viewBox
    vec2 center = vec2(24.0, 24.0);
    float period = 0.5; // adjust speed
    float t_total = time; // assume uniform time
    float angle = 2.0 * 3.14159265 * (t_total / period);
    p -= center;
    p = rotate2d(angle) * p;
    p += center;
    float cycle = mod(t_total / period, 7.0);
    float frac = fract(cycle);
    int curr = int(floor(cycle));
    vec2 poly[N];
    for(int i = 0; i < N; i++){
        vec2 a;
        if(curr == 0) a = shape1[i];
        else if(curr == 1) a = shape2[i];
        else if(curr == 2) a = shape3[i];
        else if(curr == 3) a = shape4[i];
        else if(curr == 4) a = shape5[i];
        else if(curr == 5) a = shape6[i];
        else a = shape7[i];
        int nxt = (curr + 1) % 7;
        vec2 b;
        if(nxt == 0) b = shape1[i];
        else if(nxt == 1) b = shape2[i];
        else if(nxt == 2) b = shape3[i];
        else if(nxt == 3) b = shape4[i];
        else if(nxt == 4) b = shape5[i];
        else if(nxt == 5) b = shape6[i];
        else b = shape7[i];
        poly[i] = mix(a, b, frac);
    }
    float min_d = min_dist_to_poly(p, poly);
    bool ins = is_inside(p, poly);
    float sdf = ins ? -min_d : min_d;
    float aa = fwidth(length(p)) * 1.5; // or fixed 0.5
    float alpha = 1.0 - smoothstep(0.0, aa, sdf);
    return COLOR * alpha;
}